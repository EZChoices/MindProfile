generator client {
  provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
}

model Profile {
  id                 String   @id @default(cuid())
  createdAt          DateTime @default(now())
  sourceMode         String
  confidence         String
  thinkingStyle      String
  communicationStyle String
  strengthsJson      String
  blindSpotsJson     String
  suggestedJson      String
  rawText            String
}
